{
  "name": "paper-du-jour",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "generate": "node scripts/generate-paper.js",
    "generate:clearecondl": "node scripts/generate-clearecondl.js",
    "generate:gehc2025": "node scripts/generate-gehealthcare-2025.js",
    "generate:all": "npm run generate && npm run generate:clearecondl && npm run generate:gehc2025",
    "generate:li-clearecondl": "node scripts/generate-linkedin-clearecondl.js",
    "li:debug": "cross-env LOG_LEVEL=debug DEBUG_SAVE_HTML=1 PROFILE_ONLY=1 node scripts/generate-linkedin-clearecondl.js",
    "li:profile": "cross-env PROFILE_URLS=\"https://www.linkedin.com/in/charles-nutting-do-fsir-5b18b95a/\" LOG_LEVEL=debug PROFILE_ONLY=1 node scripts/generate-linkedin-clearecondl.js",
    "li:slow": "cross-env PAUSE_MS=1800 LOG_LEVEL=debug PROFILE_ONLY=1 node scripts/generate-linkedin-clearecondl.js"
  },
  "dependencies": {
    "astro": "^4.10.0",
    "gray-matter": "^4.0.3",
    "node-fetch": "^3.3.2",
    "slugify": "^1.6.6",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
